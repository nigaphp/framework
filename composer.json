{
    "name": "nigatedev/framework-bundle",
    "description": "Nigatedev framework bundle where Nigatedev framework dependencies live!",
    "type": "library",
    "keywords": ["framework", "nigatedev", "bundle"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nigatedev\\FrameworkBundle\\": ""
        }
    },
    "authors": [
        {
            "name": "Abass Ben Cheik",
            "email": "cheikabassben@gmail.com"
        }
    ],
    "require": {
      "php": "^7.3|^8.0",
      "nigatedev/diyan": "^1.0",
      "nigatedev/support": "^1.0",
      "nigatedev/query-builder": "^1.0",
      "nigatedev/filesystem": "^1.0",
      "nigatedev/maker": "^1.0",
      "vlucas/phpdotenv": "^5.3",
      "nigatedev/console": "^1.0",
      "guzzlehttp/psr7": "^2.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12.94",
        "filp/whoops": "^2.14",
        "nigatedev/dumper": "^1.0"
    },
    "minimum-stability": "dev"
}
